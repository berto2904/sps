--------------------------------------------------------------------------------
SELECT entrevista.id_entrevista,
        entrevista.organizacion,
        entrevista.puesto,
        entrevista.fecha_hora,
        entrevista.informacion_relevante,
        entrevista.id_usuario,
        entrevista.id_postulante
FROM entrevista;
--------------------------------------------------------------------------------
SELECT postulante.id_postulante,
        postulante.nombres,
        postulante.apellido,
        postulante.fecha_de_nacimiento,
        postulante.licencia_conductor,
        postulante.lugar_nacimiento,
        postulante.nacionalidad,
        postulante.dni,
        postulante.profesion,
        postulante.licencia_categoria,
        postulante.id_informacion_socioambiental,
        postulante.id_informacion_economica,
        postulante.id_sexo,
        postulante.id_estado_civil,
        postulante.expedida_por_B
FROM postulante;
--------------------------------------------------------------------------------
SELECT sexo.id_sexo,
        sexo.descripcion
FROM sexo;
--------------------------------------------------------------------------------
SELECT estado_civil.id_estado_civil,
        estado_civil.descripcion,
        estado_civil.tipo
FROM estado_civil;
--------------------------------------------------------------------------------
SELECT conyuge.id_conyuge,
      conyuge.nombres,
      conyuge.apellido,
      conyuge.fecha_nacimiento,
      conyuge.lugar_nacimiento,
      conyuge.nacionalidad,
      conyuge.profesion,
      conyuge.dni,
      conyuge.id_postulante,
      conyuge.id_sexo
FROM conyuge;
--------------------------------------------------------------------------------
SELECT informacion_economica.id_informacion_economica,
        informacion_economica.id_movilidad_propia
FROM informacion_economica;
--------------------------------------------------------------------------------
SELECT informacion_socioambiental.id_informacion_socioambiental,
        informacion_socioambiental.id_domicilio,
        informacion_socioambiental.id_vivienda
FROM informacion_socioambiental;
--------------------------------------------------------------------------------
SELECT domicilio.id_domicilio,
        domicilio.localidad,
        domicilio.calle,
        domicilio.numero,
        domicilio.piso,
        domicilio.departamento,
        domicilio.gmap,
        domicilio.telefono,
        domicilio.referencia_util,
        domicilio.codigo_postal,
        domicilio.partido
FROM domicilio;
--------------------------------------------------------------------------------
SELECT movilidad_propia.id_movilidad_propia,
        movilidad_propia.id_vehiculo_tipo,
        movilidad_propia.marca,
        movilidad_propia.modelo,
        movilidad_propia.anio,
        movilidad_propia.titular,
        movilidad_propia.patente
FROM movilidad_propia;
--------------------------------------------------------------------------------
SELECT vehiculo_tipo.id_vehiculo_tipo,
        vehiculo_tipo.descripcion
FROM vehiculo_tipo;
--------------------------------------------------------------------------------
SELECT tarjeta_credito_debito.id_tarjeta_credito_debito,
        tarjeta_credito_debito.id_informacion_economica,
        tarjeta_credito_debito.otras_propiedades,
        tarjeta_credito_debito.seguro_de_vida,
        tarjeta_credito_debito.prendas,
        tarjeta_credito_debito.observaciones
FROM tarjeta_credito_debito;
--------------------------------------------------------------------------------
SELECT vivienda.id_vivienda,
        vivienda.id_tipo_vivienda,
        vivienda.aspecto_interior,
        vivienda.propietario,
        vivienda.ambientes,
        vivienda.aspecto_exterior,
        vivienda.inquilino,
        vivienda.importe_alquiler,
        vivienda.accesibilidad
FROM vivienda;
