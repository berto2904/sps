SELECT informe_laboral.id_informe_laboral,
        informe_laboral.id_referencias_laborales,
        informe_laboral.fecha_ingreso,
        informe_laboral.fecha_egreso,
        informe_laboral.puesto_al_ingresar,
        informe_laboral.ultimo_puesto_ocupado,
        informe_laboral.causa_de_egreso,
        informe_laboral.asistencia,
        informe_laboral.puntualidad,
        informe_laboral.concepto_general
FROM informe_laboral;
